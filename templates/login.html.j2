{% extends "header.html.j2" %}

{% block body %}
	<h1>Login</h1>
	<form action="" method="POST">
		<div class="">
			<label>Username</label>
			<input type="text" name="username" class="" value={{ request.form.username }}>
		</div>

		<div class="">
			<label>Password</label>
			<input type="password" name="password" class="" value={{ request.form.password }}>
		</div>

    	<button type="submit" class="">Submit</button>

	</form>
{% endblock %}
